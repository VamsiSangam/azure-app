			    closeMenuCtrl = document.querySelector('.action--close');
            var navOpened = true;

		    openMenuCtrl.addEventListener('click', openMenu);
		    closeMenuCtrl.addEventListener('click', closeMenu);

		    function openMenu() {
			    classie.add(menuEl, 'menu--open');
			    closeMenuCtrl.focus();
		    }

		    function closeMenu() {
			    classie.remove(menuEl, 'menu--open');
			    openMenuCtrl.focus();
                navOpened = false;
		    }

		    // simulate grid content loading
		    var gridWrapper = document.querySelector('.content');

            $('#menu-toggle-btn').click(function() {
                if (window.innerWidth >= 40 * getEmPixels()) {
                    console.log("40em = " + (40 * getEmPixels()));
                    console.log("window.innerWidth = " + window.innerWidth);

                    if (navOpened) {
                        $('#ml-menu-logo').css('margin-left', '0px');
                        $('#ml-menu').css('margin-left', '0px');
                        navOpened = false;
                    } else {
                        $('#ml-menu-logo').css('margin-left', '-300px');
                        $('#ml-menu').css('margin-left', '-300px');
                        navOpened = true;
                    }
                } else {
                    var width = -1 * window.innerWidth;

                    if (navOpened) {
                        $('#ml-menu-logo').css('margin-left', '0px');
                        $('#ml-menu').css('margin-left', '0px');
                        navOpened = false;
                    } else {
                        $('#ml-menu-logo').css('margin-left', width);
                        $('#ml-menu').css('margin-left', width);
                        navOpened = true;
                    }
                }
            });
	    })();
    </script>
